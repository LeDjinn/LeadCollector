<p id="notice"><%= notice %></p>

<h1>Contentwebs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contentwebs.each do |contentweb| %>
      <tr>
        <td><%= link_to 'Show', contentweb %></td>
        <td><%= link_to 'Edit', edit_contentweb_path(contentweb) %></td>
        <td><%= link_to 'Destroy', contentweb, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contentweb', new_contentweb_path %>
